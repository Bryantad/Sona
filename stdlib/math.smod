const PI = 3.14159;

func add(a, b) {
    return a + b
}

func sub(a, b) {
    return a - b
}

func sqrt(x) {
    let guess = x / 2;
    let i = 0;
    while (i < 10) {
        guess = (guess + x / guess) / 2;
        i = i + 1;
    }
    return guess;
}

func pow(base, exp) {
    let result = 1;
    let i = 0;
    while (i < exp) {
        result = result * base;
        i = i + 1;
    }
    return result;
}
