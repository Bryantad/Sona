// Tier 2 Features Test

// 1. Dictionaries
let person = {"name": "Alice", "age": 30};
print(person);

// 2. For loops - iterate over list
let numbers = [1, 2, 3];
for num in numbers {
    print(num);
};

// 3. For loops - range
for i in range(3) {
    print(i);
};

