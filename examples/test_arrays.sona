import utils.array.smod as array

// Test array literals
let a = [1, 2, 3]
print("Initial array:")
print(a)

// Test array module methods
print("Array length:")
print(array.length(a))

print("Push 4 to array:")
array.push(a, 4)
print(a)

print("Get element at index 2:")
print(array.get(a, 2))

print("Set element at index 1 to 10:")
array.set(a, 1, 10)
print(a)

print("Pop last element:")
print(array.pop(a))

print("Array after operations:")
print(a)

// Test array concatenation
let b = [4, 5, 6]
print("Concatenated arrays:")
print(array.concat(a, b))

// Test contains
print("Does array contain 10?")
print(array.contains(a, 10))

// Test slice
print("Slice array from index 0 to 2:")
print(array.slice(a, 0, 2))
